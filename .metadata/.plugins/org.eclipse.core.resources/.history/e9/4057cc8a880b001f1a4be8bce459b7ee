package com.hm;

import java.util.*;
public class Dev {
	int id;
	String name;
	int salary = 60000;
	String desig = "Developer";

	public Dev() {
		// TODO Auto-generated constructor stub
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter ID : ");
		id = sc.nextInt();
		System.out.println("Enter Name : ");
		name =  sc.next();
	}
	
	void display() {
		System.out.println(id + "\n"+name+"\n"+desig);
}
